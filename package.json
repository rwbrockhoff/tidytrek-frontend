{
	"name": "tt-frontend",
	"version": "1.0.0",
	"description": "Tidytrek Frontend",
	"author": "Ryan Brockhoff <ryanbrockhoff@protonmail.com>",
	"type": "module",
	"private": true,
	"license": "UNLICENSED",
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"scripts": {
		"commit": "cz",
		"dev": "vite",
		"dev:test": "NODE_ENV=test vite --port 5174",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"test": "vitest",
		"test:coverage": "vitest run --coverage --coverage.reporter=html",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"prepare": "husky"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@fontsource-variable/inter": "^5.2.6",
		"@radix-ui/react-form": "^0.1.7",
		"@radix-ui/themes": "^2.0.3",
		"@sentry/react": "^9.41.0",
		"@sentry/vite-plugin": "^4.0.0",
		"@stripe/react-stripe-js": "^4.0.0",
		"@stripe/stripe-js": "^7.9.0",
		"@supabase/supabase-js": "^2.39.7",
		"@tailwindcss/postcss": "^4.1.11",
		"@tanstack/react-query": "^5.18.0",
		"axios": "^1.6.2",
		"chart.js": "^4.4.1",
		"hashids": "^2.3.0",
		"lucide-react": "^0.525.0",
		"radix-ui": "^1.4.2",
		"react": "^18.2.0",
		"react-chartjs-2": "^5.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^4.0.13",
		"react-router": "^7.8.1",
		"react-router-dom": "^7.8.1",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@playwright/test": "^1.53.1",
		"@storybook/addon-docs": "^9.0.14",
		"@storybook/react-vite": "^9.0.14",
		"@tanstack/eslint-plugin-query": "^5.18.0",
		"@tanstack/react-query-devtools": "^5.83.0",
		"@testing-library/jest-dom": "^6.2.0",
		"@testing-library/react": "^14.1.2",
		"@testing-library/user-event": "^14.5.2",
		"@types/google-one-tap": "^1.2.6",
		"@types/node": "^20.10.5",
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"@types/react-router-dom": "^5.3.3",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"@vitejs/plugin-react": "^4.2.1",
		"@vitest/coverage-v8": "^3.2.4",
		"autoprefixer": "^10.4.21",
		"commitizen": "^4.3.1",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv": "^16.6.0",
		"eslint": "^8.55.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"husky": "^9.1.7",
		"jsdom": "^23.2.0",
		"mailslurp-client": "^15.24.1",
		"postcss": "^8.5.6",
		"prettier": "^3.2.4",
		"storybook": "^9.0.14",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.2.2",
		"vite": "^5.0.8",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^3.2.4"
	}
}
